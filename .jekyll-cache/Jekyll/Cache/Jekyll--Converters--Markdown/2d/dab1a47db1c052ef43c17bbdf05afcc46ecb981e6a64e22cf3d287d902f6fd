    <script>
        let requestURL = '/assets/json/export.json'

        // // Method 2: Use a fetch() function
        let getData = async () => {
            let res = await fetch(requestURL)
            let data = await res.json()
            // console.log(data)
            let has = []
            let keywordsArr = []
            for(let name in data){
                data[name].map(str => {
                    let keywords = str.split(' ')
                    for(let keyword of keywords){
                        if(keyword == 'ì´ëª¨í‹°ì½˜') continue
                        if(keyword == 'ì‚¬ì§„') continue
                        if(/^ì‚¬ì§„ \d+ì¥$/.test(keyword)) continue
                        if(/[ã„±ã„²ã„´ã„·ã„¸ã„¹ã…ã…‚ã…ƒã……ã…†ã…‡ã…ˆã…‰ã…Šã…‹ã…Œã…ã…!?.,;^ã…ã…‘ã…“ã…•ã…—ã…›ã…œã… ã…¡ã…£+-]+/.test(keyword)) continue
                        if(!has.includes(keyword)){
                            has.push(keyword)
                            keywordsArr.push([keyword, 0])
                        }
                        let idx = has.indexOf(keyword)
                        keywordsArr[idx][1]++
                    }
                })
            }
            // keywordsArr.sort((a,b) => b[1] - a[1])
            console.log(keywordsArr)
        }
        getData()
    </script>
